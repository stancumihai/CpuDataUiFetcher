        <Button fx:id="stopButton" layoutX="11.0" layoutY="555.0" mnemonicParsing="false" onAction="#stopUi" prefHeight="36.0" prefWidth="195.0" style="-fx-background-color: coral;" text="Stop" textFill="WHITE">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Label fx:id="temperature" alignment="CENTER" layoutX="4.0" layoutY="332.0" prefHeight="63.0" prefWidth="210.0" text="                 " textAlignment="CENTER">
